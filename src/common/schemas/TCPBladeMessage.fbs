namespace cirrus.message.TCPBladeMessage;

table Write{
  oid:ulong;
  data:[byte];
}

table WriteAck{
  oid:ulong;
  success:byte;
}

table Read{
  oid:ulong;
}

table ReadAck{
  oid:ulong;
  success:byte;
  data:[byte];
}

table Remove{
  oid:ulong;
}

table RemoveAck{
  oid:ulong;
  success:byte;
}

table TCPBladeMessage {
  txnid:ulong (required);
  error_code:long (required);
  message:Message (required);
}

root_type TCPBladeMessage;
